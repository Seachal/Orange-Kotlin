<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
            name="viewModel"
            type="com.zhuzichu.orange.ui.mine.setting.theme.viewmodel.ViewModelTheme" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.zhuzichu.base.widget.NiceToolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:onClickNavigation="@{viewModel.onClickNavigation}"
            app:toolbar_navigation_icon="@drawable/ic_back"
            app:toolbar_title="@string/theme" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>